{
    "name": "GetYTChannelRSSURL",
    "description": "Get RSS URL from YouTube Channels!",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/c/*", "https://*.youtube.com/channel/*"],
            "js": ["content-script.js"]
        }
    ]
}
