{
    "name": "Get YouTube RSS Feed",
    "description": "Get RSS URL from YouTube Channels!",
    "version": "1.0.1",
    "manifest_version": 3,
    "author": "Carlos Amorim",
    "permissions": ["activeTab", "tabs", "scripting"],
    "action": {
            "default_popup": "index.html",
            "default_title": "Chanel RSS Feed"
    },
   "web_accessible_resources": [{
        "resources": ["popup-script.js"],
        "matches": ["https://*.youtube.com/*"]
    }]
}
